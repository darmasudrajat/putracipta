                                <td>{{ form_row(form.subconMagentaRevisionEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                            </tr>
                            <tr>
                                <td class="text-center">Y</td>
                                <td>{{ form_row(form.subconYellowBeginningOutputStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.subconYellowBeginningOutputEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.subconYellowRevisionStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.subconYellowRevisionEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                            </tr>
                            <tr>
                                <td class="text-center">B</td>
                                <td>{{ form_row(form.subconBlackBeginningOutputStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.subconBlackBeginningOutputEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.subconBlackRevisionStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.subconBlackRevisionEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </fieldset>

        <fieldset class="border border-secondary rounded mb-3 p-3">
            <div class="row">
                <div class="col">
                    {{ form_row(form.quantityPlateNew, {'label': 'Plate Baru'}) }}
                    {{ form_row(form.quantityPlateOld, {'label': 'Plate Lama'}) }}
                    {{ form_row(form.quantityPlateUsed, {'label': 'Plate Bekas'}) }}
                    {{ form_row(form.plateBrand, {'label': 'Plate Merk'}) }}
                </div>
                <div class="col">
                    {{ form_row(form.employeeIdPlateRelease, {'label': 'Plate Diserahkan kepada'}) }}
                    {{ form_row(form.employeeIdWorkOrderReturn, {'label': 'WO dikembalikan oleh'}) }}
                    {{ form_row(form.workOrderReturnDate, {'label': 'Tanggal Pengembalian WO', 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}
                </div>
            </div>
        </fieldset>

        <fieldset class="border border-secondary rounded mb-3 p-3">
            <div class="row">
                <div class="col">
                    {{ form_row(form.note, {'label': 'Catatan'}) }}
                </div>
            </div>
        </fieldset>

        <div class="d-grid">
            <button class="btn btn-success"
                    data-controller="dom-element"
                    data-action="dom-element#confirm"
                    data-dom-element-confirm-message-param="Are you sure you want to save this transaction?">
                Save
            </button>
            <input type="hidden" name="{{ idempotent_token_name() }}" value="{{ idempotent_token_value() }}" />
        </div>
    {{ form_end(form) }}

