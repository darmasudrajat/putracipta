<div data-controller="event-emitter json-loader"
     data-action="data-table:select->event-emitter#forward"
     data-event-emitter-attributes-list-param="{{ {'data-table:select': 'id'}|json_encode }}"
     data-json-loader-url-value="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({'_format': 'json'})) }}"
     data-json-loader-method-value="POST"
     data-json-loader-form-target-value="{{ "form[name=#{form.vars.name}]" }}">

    {{ form_start(form) }}
        <fieldset class="border border-secondary rounded mb-3 p-3">
            <legend class="float-none w-auto text-dark px-1">Header</legend>
            <div class="row">
                <div class="col">
                    {{ form_row(form.transactionDate, {'label': 'Tanggal Produksi', 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}
                    <div class="mb-3">
                        <label class="form-label">Mesin Cetak</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.printingMachine|default }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderPrintingMachine'}},
                               ]|json_encode }}" />
                    </div>
                    <div class="mb-3">
                        <label class="form-label">Customer</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.customer.company|default }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.customerCompany'}},
                               ]|json_encode }}" />
                    </div>
                    <div class="mb-3">
                        <label class="form-label">Kode Design</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.designCode|default }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderDesignCode'}},
                               ]|json_encode }}" />
                    </div>
                    <div class="mb-3">
                        <label class="form-label">Kode Produk</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.product.code|default }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.productCode'}},
                               ]|json_encode }}" />
                    </div>
                    <div class="mb-3">
                        <label class="form-label">Nama Produk</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.product.name|default }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.productName'}},
                               ]|json_encode }}" />
                    </div>
                    <div class="mb-3">
                        <label class="form-label">Quantity Order</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.quantityOrder|default }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderQuantityOrder'}},
                               ]|json_encode }}" />
                    </div>
                    
                    <div class="mb-3">
                        <label class="form-label">Stock / Kekurangan (pcs)</label>
                        <div class="row">
                            <div class="col">
                                <div class="input-group">
                                    <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.quantityStock|default }}" readonly="readonly"
                                       data-controller="dom-element"
                                       data-action="master-order-table:data-table:select@document->dom-element#bind"
                                       data-dom-element-bind-specifications-param="{{ [
                                           {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderQuantityStock'}},
                                       ]|json_encode }}" />
                                    <span class="input-group-text">/</span>
                                    <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.quantityProduction|default }}" readonly="readonly"
                                       data-controller="dom-element"
                                       data-action="master-order-table:data-table:select@document->dom-element#bind"
                                       data-dom-element-bind-specifications-param="{{ [
                                           {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderQuantityProduction'}},
                                       ]|json_encode }}" />
                                </div>
                            </div>
                        </div>
                    </div>
                        
                    <div class="mb-3">
                        <label class="form-label">Ukuran Jadi (mm)</label>
                        <div class="row">
                            <div class="col">
                                <div class="input-group">
                                    <span class="input-group-text">P</span>
                                    <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.dimensionLength|default }}" readonly="readonly"
                                       data-controller="dom-element"
                                       data-action="master-order-table:data-table:select@document->dom-element#bind"
                                       data-dom-element-bind-specifications-param="{{ [
                                           {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderDimensionLength'}},
                                       ]|json_encode }}" />
                                    <span class="input-group-text">L</span>
                                    <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.dimensionWidth|default }}" readonly="readonly"
                                       data-controller="dom-element"
                                       data-action="master-order-table:data-table:select@document->dom-element#bind"
                                       data-dom-element-bind-specifications-param="{{ [
                                           {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderDimensionWidth'}},
                                       ]|json_encode }}" />
                                    <span class="input-group-text">T</span>
                                    <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.dimensionHeight|default }}" readonly="readonly"
                                       data-controller="dom-element"
                                       data-action="master-order-table:data-table:select@document->dom-element#bind"
                                       data-dom-element-bind-specifications-param="{{ [
                                           {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderDimensionHeight'}},
                                       ]|json_encode }}" />
                                </div>
                            </div>
                        </div>
                    </div>
                            
                    <div class="mb-3">
                        <label class="form-label">Warna / Pantone</label>
                        <div class="row">
                            <div class="col">
                                <div class="input-group">
                                    <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.color|default }}" readonly="readonly"
                                       data-controller="dom-element"
                                       data-action="master-order-table:data-table:select@document->dom-element#bind"
                                       data-dom-element-bind-specifications-param="{{ [
                                           {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderColor'}},
                                       ]|json_encode }}" />
                                    <span class="input-group-text">/</span>
                                    <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.pantone|default }}" readonly="readonly"
                                       data-controller="dom-element"
                                       data-action="master-order-table:data-table:select@document->dom-element#bind"
                                       data-dom-element-bind-specifications-param="{{ [
                                           {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderPantone'}},
                                       ]|json_encode }}" />
                                    <span class="input-group-text">+</span>
                                    <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.colorPantoneAdditional|default }}" readonly="readonly"
                                       data-controller="dom-element"
                                       data-action="master-order-table:data-table:select@document->dom-element#bind"
                                       data-dom-element-bind-specifications-param="{{ [
                                           {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderColorPantoneAdditional'}},
                                       ]|json_encode }}" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                                
                <div class="col">
                    <div class="mb-3">
                        <label for="{{ form.masterOrderHeader.vars.id }}-text" class="form-label">Master Order #</label>
                        {{ form_row(form.masterOrderHeader, {
                            'attr': {
                                'data-controller': 'dom-element',
                                'data-action': 'master-order-table:data-table:select@document->dom-element#bind',
                                'data-dom-element-bind-specifications-param': [
                                    {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderId'}},
                                ]|json_encode,
                            },
                        }) }}
                        <input type="text" id="{{ form.masterOrderHeader.vars.id }}-text" class="form-control readonly-form-input {% if not form.masterOrderHeader.vars.valid %}is-invalid{% endif %}" value="{{ workOrderPrepress.masterOrderHeader.codeNumber|default }}" readonly="readonly"
                               data-bs-toggle="modal" data-bs-target="#master-order-dialog"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderCodeNumber'}},
                               ]|json_encode }}" />
                        {{ form_errors(form.masterOrderHeader) }}
                    </div>
                    <div class="mb-3">
                        <label class="form-label">No. PO</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.saleOrderDetail.saleOrderHeader.referenceNumber|default }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.saleOrderHeaderReferenceNumber'}},
                               ]|json_encode }}" />
                    </div>
                    <div class="mb-3">
                        <label class="form-label">Tanggal MO</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.transactionDate|default ? workOrderPrepress.masterOrderHeader.transactionDate|date('d M Y') : '' }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderDate'}},
                               ]|json_encode }}" />
                    </div>
                    <div class="mb-3">
                        <label class="form-label">Tanggal PO</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.saleOrderDetail.saleOrderHeader.transactionDate|default ? workOrderPrepress.masterOrderHeader.saleOrderDetail.saleOrderHeader.transactionDate|date('d M Y') : '' }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.saleOrderHeaderTransactionDate'}},
                               ]|json_encode }}" />
                    </div>
                    <div class="mb-3">
                        <label class="form-label">Tanggal Kirim</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.deliveryDate|default ? workOrderPrepress.masterOrderHeader.deliveryDate|date('d M Y') : '' }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderDeliveryDate'}},
                               ]|json_encode }}" />
                    </div>
                    <div class="mb-3">
                        <label class="form-label">Status Cetakan</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.printingStatus|default }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderPrintingStatus'}},
                               ]|json_encode }}" />
                    </div>
                    <div class="mb-3">
                        <label class="form-label">Finishing</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.finishing|default }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderFinishing'}},
                               ]|json_encode }}" />
                    </div>
                    
                    <div class="mb-3">
                        <label class="form-label">Material Kertas</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.paperFep|default }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderPaper'}},
                               ]|json_encode }}" />
                    </div>
                    
                    <div class="mb-3">
                        <label class="form-label">Jumlah Up Cetak</label>
                        <div class="row">
                            <div class="col">
                                <div class="input-group">
                                    <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.quantityPrinting|default }}" readonly="readonly"
                                       data-controller="dom-element"
                                       data-action="master-order-table:data-table:select@document->dom-element#bind"
                                       data-dom-element-bind-specifications-param="{{ [
                                           {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderQuantityPrinting'}},
                                       ]|json_encode }}" />
                                    <span class="input-group-text">&</span>
                                    <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.quantityPrinting2|default }}" readonly="readonly"
                                       data-controller="dom-element"
                                       data-action="master-order-table:data-table:select@document->dom-element#bind"
                                       data-dom-element-bind-specifications-param="{{ [
                                           {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderQuantityPrinting2'}},
                                       ]|json_encode }}" />
                                    <span class="input-group-text">Up/S</span>
                                </div>
                            </div>
                        </div>
                    </div>
                                
                    <div class="mb-3">
                        <label class="form-label">Ukuran Mountage/Kris</label>
                        <input type="text" class="form-control" value="{{ workOrderPrepress.masterOrderHeader.mountageSizeLength|default }}" readonly="readonly"
                               data-controller="dom-element"
                               data-action="master-order-table:data-table:select@document->dom-element#bind"
                               data-dom-element-bind-specifications-param="{{ [
                                   {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderMountageSizeLength'}},
                               ]|json_encode }}" />
                    </div>
                </div>
            </div>
        </fieldset>

        <fieldset class="border border-secondary rounded mb-3 p-3">
            <div class="row">
                <div class="table-responsive">
                    <table class="table table-bordered">
                        <thead class="table-secondary">
                            <tr>
                                <th rowspan="2">&nbsp;</th>
                                <th rowspan="2" class="text-center">Warna</th>
                                <th colspan="2" class="text-center">Output Awal</th>
                                <th colspan="2" class="text-center">Tambahan / Revisi</th>
                                <th colspan="3" class="text-center">Downtime</th>
                            </tr>
                            <tr>
                                <th class="text-center">Mulai</th>
                                <th class="text-center">Selesai</th>
                                <th class="text-center">Mulai</th>
                                <th class="text-center">Selesai</th>
                                <th class="text-center">Mulai</th>
                                <th class="text-center">Selesai</th>
                                <th class="text-center">Keterangan</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td rowspan="4">CTcP</td>
                                <td class="text-center">C</td>
                                <td>{{ form_row(form.internalCyanBeginningOutputStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalCyanBeginningOutputEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalCyanRevisionStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalCyanRevisionEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalCyanDowntimeStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalCyanDowntimeEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalCyanDowntimeMemo, {'label': false}) }}</td>
                            </tr>
                            <tr>
                                <td class="text-center">M</td>
                                <td>{{ form_row(form.internalMagentaBeginningOutputStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalMagentaBeginningOutputEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalMagentaRevisionStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalMagentaRevisionEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalMagentaDowntimeStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalMagentaDowntimeEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalMagentaDowntimeMemo, {'label': false}) }}</td>
                            </tr>
                            <tr>
                                <td class="text-center">Y</td>
                                <td>{{ form_row(form.internalYellowBeginningOutputStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalYellowBeginningOutputEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalYellowRevisionStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalYellowRevisionEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalYellowDowntimeStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalYellowDowntimeEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalYellowDowntimeMemo, {'label': false}) }}</td>
                            </tr>
                            <tr>
                                <td class="text-center">B</td>
                                <td>{{ form_row(form.internalBlackBeginningOutputStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalBlackBeginningOutputEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalBlackRevisionStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalBlackRevisionEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalBlackDowntimeStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalBlackDowntimeEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.internalBlackDowntimeMemo, {'label': false}) }}</td>
                            </tr>
                            <tr>
                                <td rowspan="4">Plate Subcon</td>
                                <td class="text-center">C</td>
                                <td>{{ form_row(form.subconCyanBeginningOutputStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.subconCyanBeginningOutputEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.subconCyanRevisionStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.subconCyanRevisionEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                            </tr>
                            <tr>
                                <td class="text-center">M</td>
                                <td>{{ form_row(form.subconMagentaBeginningOutputStartDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
                                <td>{{ form_row(form.subconMagentaBeginningOutputEndDate, {'label': false, 'attr': {'data-controller': 'flatpickr-element', 'class': 'readonly-form-input'}}) }}</td>
