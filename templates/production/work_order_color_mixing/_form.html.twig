        <fieldset class="border border-secondary rounded mb-3 p-3">
            <legend class="float-none w-auto text-dark px-1">Estimasi Tinta / Chemical</legend>
            <div class="row">
                <div class="col">
                    <div class="mb-3">
                        <label class="form-label">Cyan</label>
                        <div class="row">
                            <div class="col">
                                <div class="input-group">
                                    <input type="text" class="form-control" value="{{ workOrderColorMixing.masterOrderHeader.inkCyanPercentage|default }}" readonly="readonly"
                                       data-controller="dom-element"
                                       data-action="master-order-table:data-table:select@document->dom-element#bind"
                                       data-dom-element-bind-specifications-param="{{ [
                                           {'action': 'setPropertyValue', 'descriptor': {'property': 'value', 'value': '$event.detail.masterOrderHeaderCuttingLengthSize2'}},
