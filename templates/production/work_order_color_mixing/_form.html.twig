            <legend class="float-none w-auto text-dark px-1">Realisasi Pemakaian Inseet Kertas</legend>
            <div class="row">
                <div class="col">
                    {{ form_row(form.paperInseetUsedUsage, {'label': 'Kertas Bekas (Dreek)'}) }}
                </div>
                <div class="col">
                    {{ form_row(form.paperInseetNewUsage, {'label': 'Kertas Baru (Dreek)'}) }}
                </div>
            </div>
