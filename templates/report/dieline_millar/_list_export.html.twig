<table>
    <thead>
        <tr>
            <th style="text-align: center" colspan="7"><strong>PT. Putracipta Karindomas</strong></th>
        </tr>
        <tr>
            <th style="text-align: center" colspan="7"><strong>Laporan Millar</strong></th>
        </tr>
        <tr>
            <th style="text-align: center" colspan="7">
                <strong>
                    {% set startDate = form.vars.value.filter.date.1|default %}
                    {% set endDate = form.vars.value.filter.date.2|default %}
                    {{ startDate ? startDate|date('j F Y') }} &nbsp;&ndash;&nbsp; {{ endDate ? endDate|date('j F Y') }}
                </strong>
            </th>
        </tr>
        <tr>
            <th colspan="7"></th>
        </tr>
        <tr>
            <th style="text-align: center; width: 100pt"><strong>Customer</strong></th>
            <th style="text-align: center; width: 100pt"><strong>Kode Millar</strong></th>
            <th style="text-align: center; width: 100pt"><strong>Tanggal Pembuatan</strong></th>
            <th style="text-align: center; width: 100pt"><strong>Jml Up Cetak</strong></th>
            <th style="text-align: center; width: 100pt"><strong>Layout Cetak</strong></th>
            <th style="text-align: center; width: 100pt"><strong>Note</strong></th>
            <th style="text-align: center; width: 100pt"><strong>Kode Produk</strong></th>
            <th style="text-align: center; width: 100pt"><strong>Nama Produk</strong></th>
            <th style="text-align: center; width: 100pt"><strong>Description</strong></th>
        </tr>
        <tr>
            <th colspan="7"></th>
        </tr>
    </thead>
    <tbody>
        {% for dielineMillar in dielineMillars %}
            {% for dielineMillarDetail in dielineMillar.dielineMillarDetails %}
            <tr>
                <td style="text-align: left">{{ dielineMillar.customer.company|default }}</td>
                <td style="text-align: left">{{ dielineMillar.codeNumber|default }}</td>
                <td style="text-align: left">{{ dielineMillar.date|default ? dielineMillar.date|date('j M Y') }}</td>
                <td style="text-align: left">{{ dielineMillar.quantityUpPrinting|default }}</td>
                <td style="text-align: left">{{ dielineMillar.printingLayout|default }}</td>
                <td style="text-align: left">{{ dielineMillar.note|default }}</td>
                    <td style="text-align: left">{{ dielineMillarDetail.product.code|default }}</td>
                    <td style="text-align: left">{{ dielineMillarDetail.product.name|default }}</td>
                    <td style="text-align: left">{{ dielineMillarDetail.product.productLengthWidthHeightCombination|default }}</td>
                </tr>
            {% endfor %}
        {% endfor %}
    </tbody>
</table>