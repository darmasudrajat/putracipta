{
    "adjustmentStockMaterialDetails": {
        {%- for adjustmentStockMaterialDetailForm in form.adjustmentStockMaterialDetails -%}
            {%- set adjustmentStockMaterialDetail = adjustmentStockMaterialDetailForm.vars.data -%}
            "{{ adjustmentStockMaterialDetailForm.vars.name }}": {
                "quantityCurrent": "{{ adjustmentStockMaterialDetail.quantityCurrent|default|number_format(2) }}",
                "quantityDifference": "{{ adjustmentStockMaterialDetail.quantityDifference|default|number_format(2) }}"
            }
            {%- if not loop.last -%},{%- endif -%}
        {%- endfor -%}
    },
    "adjustmentStockPaperDetails": {
        {%- for adjustmentStockPaperDetailForm in form.adjustmentStockPaperDetails -%}
            {%- set adjustmentStockPaperDetail = adjustmentStockPaperDetailForm.vars.data -%}
            "{{ adjustmentStockPaperDetailForm.vars.name }}": {
                "quantityCurrent": "{{ adjustmentStockPaperDetail.quantityCurrent|default|number_format(2) }}",
                "quantityDifference": "{{ adjustmentStockPaperDetail.quantityDifference|default|number_format(2) }}"
            }
            {%- if not loop.last -%},{%- endif -%}
        {%- endfor -%}
    }
}
